import java.util.Scanner;

public class Solution {

    public static void main(String[] args) {
            Scanner sc=new Scanner(System.in);
            System.out.println("================================");
            for(int i=0;i<3;i++){
                String s1=sc.next();
                int x=sc.nextInt();
                //Complete this line
                int diff = 18-s1.length();
                System.out.printf(s1);
                if(x <10)
                {
                    String formatted = "00" + x;
                    System.out.printf("%"+diff+"s \n",formatted);    
                }
                else if (x <100)
                {
                    String formatted = "0" + x;
                    System.out.printf("%"+diff+"s \n",formatted);    
                }
                else    
                {
                    System.out.printf("%"+diff+"s \n",x);
                }
            }
            System.out.println("================================");

    }
}
