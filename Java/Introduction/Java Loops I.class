import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int N = in.nextInt();
        int tableAux = 1;
        for(int i=N; tableAux<11; i++)
        {
            System.out.print(N + " x " + tableAux + " = " + (N * tableAux) + "\n");
            tableAux++;
        }
    }
}
